<!-- 地图 -->
<map
  id="map"
  longitude="{{curLongitude}}"
  latitude="{{curLatitude}}"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 100vh;"
></map>
